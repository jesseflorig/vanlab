    {
        "id": "2ffea9dc5d1dd5db",
        "type": "function",
        "z": "c75c795fb5b7933d",
        "name": "msg -> influx",
        "func": "// destructure the message\nconst {topic, payload } = msg;\n\nconst topicArray = topic.split('/');\n\n// sanitize topic \nconst measurement = topicArray.join('_');\n\n// break topic into tags\nconst tag = {\n    location: topicArray[0],\n    zone: topicArray[1],\n    room: topicArray[2],\n    device: topicArray[3]\n};\n\n// break payload into fields\nconst field = {}\nObject.keys(payload).map(key => {\n    field[key] = payload[key]; \n});\n\nconst timestamp = Date.now();\n\n// return new msg\nreturn {\n    measurement,\n    tag,\n    tags: tag,\n    payload: {\n        tag,\n        tags: tag,\n        field,\n        timestamp\n    }\n};",
        "outputs": 1,
        "noerr": 0,
        "initialize": "",
        "finalize": "",
        "libs": [],
        "x": 290,
        "y": 100,
        "wires": [
            [
                "808f070b71c17e3f"
            ]
        ]
    },
